@model Fall2024_Assignment3_akspencer1.Models.Actor

<form asp-action="Create" method="post" class="mb-4">
    <h4>Create New Actor</h4>
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ImdbLink"></label>
        <input asp-for="ImdbLink" class="form-control" />
        <span asp-validation-for="ImdbLink" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Gender"></label>
        <input asp-for="Gender" class="form-control" />
        <span asp-validation-for="Gender" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Age"></label>
        <input asp-for="Age" class="form-control" />
        <span asp-validation-for="Age" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="PhotoUrl"></label>
        <input asp-for="PhotoUrl" class="form-control" />
        <span asp-validation-for="PhotoUrl" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>